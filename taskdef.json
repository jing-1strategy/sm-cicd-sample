{
    "executionRoleArn": "arn:aws:iam::486790186775:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "cicd-demo",
            "image": "<IMAGE1_NAME>",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 5000,
                    "protocol": "tcp",
                    "containerPort": 5000
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "cicd-demo"
}